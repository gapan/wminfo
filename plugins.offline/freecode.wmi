#!/bin/bash

# wminfo plugin: program submissions from freecode.com (formerly freshmeat.net)

# command: wminfo -p freecode.wmi -b#184860 -f#ffffff

lynx --source localhost/wminfo/freecode.html | \
	sed 's/\n/ /g;s/</\n</g' | \
	grep 'a href="/projects/' | grep 'title=' | \
	head -n 25 | \
	sed 's/<.*>//;' | \
	punctuation
