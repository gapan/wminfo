#!/bin/bash

# wminfo plugin: headlines from slashdot.org

# command: wminfo -p slashdot.wmi -b#005e5e -f#ffffff

lynx --source localhost/wminfo/slashdot.html | \
    sed 's/\n/ /g;s/</\n</g' | \
    grep '/story/' | grep 'onclick' | \
    sed 's/<.*>//;' | \
    grep -v '^Watch$' | \
    punctuation
